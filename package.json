{
  "name": "ai-longevity-coach",
  "version": "1.0.0",
  "description": "An intelligent, AI-powered personal longevity coach with advanced analytics, RAG integration, and proactive health guidance",
  "author": "Divakar Saini",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-longevity-coach.git"
  },
  "keywords": [
    "ai",
    "health",
    "longevity",
    "coaching",
    "wellness",
    "nextjs",
    "typescript",
    "openai",
    "supabase",
    "rag",
    "mcp"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:performance": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "docker:build": "docker build -t divakar-longevity-coach .",
    "docker:run": "docker run -p 3000:3000 divakar-longevity-coach",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:scan": "docker scan divakar-longevity-coach",
    "k8s:apply": "kubectl apply -f kubernetes/",
    "k8s:delete": "kubectl delete -f kubernetes/",
    "k8s:status": "kubectl get pods -n longevity-coach",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "monitor": "curl -f http://localhost:3000/api/monitoring/apm || exit 1",
    "security": "curl -f http://localhost:3000/api/security/events || exit 1",
    "logs": "curl -f http://localhost:3000/api/monitoring/logs || exit 1",
    "deploy:staging": "vercel --env staging",
    "deploy:production": "vercel --prod",
    "deploy:k8s": "kubectl apply -f kubernetes/ && kubectl rollout status deployment/longevity-coach -n longevity-coach",
    "rollback": "kubectl rollout undo deployment/longevity-coach -n longevity-coach",
    "scale:up": "kubectl scale deployment longevity-coach --replicas=5 -n longevity-coach",
    "scale:down": "kubectl scale deployment longevity-coach --replicas=1 -n longevity-coach",
    "backup": "kubectl get all -n longevity-coach -o yaml > backup-$(date +%Y%m%d-%H%M%S).yaml",
    "security:scan": "npm audit && docker scan divakar-longevity-coach",
    "performance:test": "npm run test:performance && npm run test:e2e",
    "production:check": "npm run health && npm run monitor && npm run security && npm run logs"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "openai": "^6.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
